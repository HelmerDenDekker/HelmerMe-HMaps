<Window x:Class="HMaps.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HMaps.Wpf"
        xmlns:source="clr-namespace:HMaps.Wpf.Source"
        mc:Ignorable="d"
        Title="HMaps" Height="450" Width="800">
    <Grid>
        <GroupBox Name="MapGroup" Header="gmap" Margin="12,7,241,12" VerticalContentAlignment="Stretch"
                  HorizontalContentAlignment="Stretch">
            <source:Map x:Name="MainMap" Zoom="13" MaxZoom="24" MinZoom="1" />
        </GroupBox>

    </Grid>
</Window>
